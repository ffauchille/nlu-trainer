{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./built",
    "removeComments": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  }
}